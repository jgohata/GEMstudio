	public updateTemp(int Temp)
	{
		new int gbByte, rgByte;
		
		rgByte = Temp * 2;	
		rgByte &amp;= 0xff;
		gbByte = 0xff - rgByte;
		
		if (Temp &lt; 0x80) 
		{
           Amulet:InternalRAM.color(1).setRed(rgByte);
           Amulet:InternalRAM.color(1).setGreen(rgByte);
           Amulet:InternalRAM.color(1).setBlue(0xFF);
		}
		else
		{
           Amulet:InternalRAM.color(1).setRed(0xFF);
           Amulet:InternalRAM.color(1).setGreen(gbByte);
           Amulet:InternalRAM.color(1).setBlue(gbByte);
       } 
        
       Amulet:document.CustomSliderField2 = Temp
	}
&lt;/script&gt;

