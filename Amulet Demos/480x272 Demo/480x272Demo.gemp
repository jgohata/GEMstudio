	public Charge()
	{
		new int val;
		val = document.BarGraph1
		new int temp1 = 0, temp2 = 0, temp3 = 0;
		if(val &lt; 255)
		{
			val += 5;
			temp1 = val
			temp2 = val
			if(val &lt; 0x80)
			{
				temp1 *= 2
				temp2 = 0xFF
				internalRAM.color(10).setGreen(temp1);
				internalRAM.color(10).setRed(temp2);
			}
			else if (val &gt; 0x7F)
			{
				temp1 = 0xFF
				temp3 = 0xFF - (val * 2)
				internalRAM.color(10).setGreen(temp1);
				internalRAM.color(10).setRed(temp3);
			}
		}
			
		return val
	}
	
