	@load()
	{
		if (InternalRAM.byte(10) == 0)
		{
			document.Release.reappear()
			showLevel()
		}
		else
		{
			document.ReadyToLevel.reappear()
			showDegrees()
		}
	}
	public Releaser()
	{
		new int F2R = internalRAM.byte(10)++
		new int L2R = internalRAM.byte(11)++
		updateOutputs()
		if(L2R &gt; 60)
		{
			document.ReleaseTimer.setUpdateRate(0)
			document.JumpTimer.setUpdateRate(200)
		}
	}
	public Leveler(){
		new int F2R = internalRAM.byte(10)--
		new int L2R = internalRAM.byte(11)--
		updateOutputs()
		if(L2R == 0)
		{
			document.LevelerTimer.setUpdateRate(0)
			showLevel()
			document.JumpTimer.setUpdateRate(200)
		}
	}
	updateOutputs()
	{
		document.F2R.forceUpdate()
		document.F2R_ones.forceUpdate()
		document.F2R_tens.forceUpdate()
		document.L2R_ones.forceUpdate()
		document.L2R_tens.forceUpdate()
		document.L2R.forceUpdate()
	}
	public showDegrees()
	{
		document.Level.disappear()
		document.F2R_ones.reappear()
		document.F2R_tens.reappear()
		document.decimal_point_1.reappear()
		document.degrees_1.reappear()
		
		document.Level_2.disappear()
		document.L2R_ones.reappear()
		document.L2R_tens.reappear()
		document.decimal_point_2.reappear()
		document.degrees_2.reappear()
	}
	
	showLevel()
	{
		document.F2R_ones.disappear()
		document.F2R_tens.disappear()
		document.decimal_point_1.disappear()
		document.degrees_1.disappear()
		document.Level.reappear()
		
		document.L2R_ones.disappear()
		document.L2R_tens.disappear()
		document.decimal_point_2.disappear()
		document.degrees_2.disappear()
		document.Level_2.reappear()
	}
	
&lt;/SCRIPT&gt;

