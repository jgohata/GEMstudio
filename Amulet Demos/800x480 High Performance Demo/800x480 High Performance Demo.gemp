static int time = 0;
static int start_y = 0
static int start_x = 0
static int icon = 0
static int offset_x = 0
static int offset_y = 0
#define INVALID_TOUCH 0xFFFF8000
#define HVAC_Resting_Y 110
#define HVAC_Resting_X 67
#define ICON_HEIGHT_DIV2 73
#define ICON_WIDTH 146
#define ICON_HEIGHT 146
#define MAX_SPRING_DISTANCE_Y 100
#define MAX_SPRING_DISTANCE_X 100
#define MAX_MAGNITUDE 100.0
public boing()
{
	new float magnitude_x = float(start_x  - HVAC_Resting_X - ICON_HEIGHT_DIV2)
	new float magnitude_y = float(start_y  - HVAC_Resting_Y - ICON_HEIGHT_DIV2)
	
	if(floatabs(magnitude_y) &gt; MAX_MAGNITUDE)
		if(magnitude_y &gt; 0.0)
			magnitude_y = MAX_MAGNITUDE
		else
			magnitude_y = -MAX_MAGNITUDE
	if(floatabs(magnitude_x) &gt; MAX_MAGNITUDE)
		if(magnitude_x &gt; 0.0)
			magnitude_x = MAX_MAGNITUDE
		else
			magnitude_x = -MAX_MAGNITUDE
		
	if ((floatabs(magnitude_y) &lt; 5.0) &amp;&amp; (floatabs(magnitude_x) &lt; 5.0))
		time = 354

	new int y_offset = floatround(magnitude_y * damping[time]) + HVAC_Resting_Y
	new int x_offset = floatround(magnitude_x * damping[time]) + HVAC_Resting_X
	
	disableDraw()
	document.HVAC_Touch.disappear()
	document.HVAC.setY(y_offset)
	document.HVAC.setX(x_offset)
	refreshRect(0,10,457,346)
	enableDraw()
	
	
	drawLineAAW(-20, 100+ICON_HEIGHT_DIV2, x_offset, y_offset+ICON_HEIGHT_DIV2) 
    drawLineAAW(457, 166, x_offset+ICON_WIDTH, y_offset+ICON_HEIGHT_DIV2)
	
	time+=10
	if(time &gt;= 355)
	{
		document.startBoing.setUpdateRate(0)
		document.HVAC_Touch.setY(y_offset)
		document.HVAC_Touch.setX(x_offset)
		document.HVAC_Touch.reappear()
	}
}
public reset()
{
	time= 0
	boing
	document.startBoing.setUpdateRate(0)
}
static int doBounce = 0
public initTracking()
{
	start_x = PenX()
	offset_x = PenX() - HVAC_Resting_X - ICON_HEIGHT_DIV2
	start_y = PenY()
	offset_y = PenY() - HVAC_Resting_Y - ICON_HEIGHT_DIV2
	//document.MyNumField_1 = offset
	doBounce = 0
}
public trackingDelay()
{
	//dont follow finger until several pixels have been swiped.
	new int temp_x = PenX()
	new int temp_y = PenY()
	if(!((temp_y &lt; (start_y+10)) &amp;&amp; (temp_y &gt; (start_y -10))) || !((temp_x &lt; (start_x+10)) &amp;&amp; (temp_x &gt; (start_x -10))))
	{
		time= 0
		doBounce = 1
		start_y = 0xFFFF //force repaint at tracking position.
		document.startTracking.setUpdateRate(4)
		document.startTrackingDelay.setUpdateRate(0)
	}
	else
		document.HVAC_Touch.reappear()
}
public checkForBounce()
{
	//document.MyNumField_1 = document.MyNumField_1 +1
	if(doBounce == 1)
	{
		document.startBoing.setUpdateRate(4)
	}
	else
	{
		Thermostat.open()
	}
	document.startCheckForBounce.setUpdateRate(0)
}
public trackFinger()
{
	new int newPosition_x = PenX()
	new int newPosition_y = PenY()
	if(newPosition_y == INVALID_TOUCH || newPosition_x == INVALID_TOUCH)
		return  //invalid touch.
	new int origPosition_y = HVAC_Resting_Y
	new int origPosition_x = HVAC_Resting_X
	//no need to draw old data
	if((start_y != newPosition_y) ||  (start_x != newPosition_x))
	{
		//make sure x or y is in bounds.
		//if(((newPosition_y &lt; (origPosition_y+MAX_SPRING_DISTANCE_Y+ICON_HEIGHT_DIV2+offset_y)) &amp;&amp; (newPosition_y &gt; (origPosition_y-MAX_SPRING_DISTANCE_Y+ICON_HEIGHT_DIV2+offset_y))) || ((newPosition_x &lt; (origPosition_x+MAX_SPRING_DISTANCE_X+ICON_HEIGHT_DIV2+offset_x)) &amp;&amp; (newPosition_x &gt; (origPosition_x-MAX_SPRING_DISTANCE_X+ICON_HEIGHT_DIV2+offset_x))))
		{
			//calculate y coordinate
			new int pos_y = newPosition_y-ICON_HEIGHT_DIV2-offset_y
			//make sure y is within bounds
			if(pos_y &lt; origPosition_y - MAX_SPRING_DISTANCE_Y)
				pos_y = origPosition_y - MAX_SPRING_DISTANCE_Y 
			else if(pos_y &gt; origPosition_y + MAX_SPRING_DISTANCE_Y)
				pos_y = origPosition_y + MAX_SPRING_DISTANCE_Y
			//calculate x coordinate
			new int pos_x = newPosition_x-ICON_HEIGHT_DIV2-offset_x
			//make sure x is within bounds
			if(pos_x &lt; origPosition_x - MAX_SPRING_DISTANCE_X)
				pos_x = origPosition_x - MAX_SPRING_DISTANCE_X 
			else if(pos_x &gt; origPosition_x + MAX_SPRING_DISTANCE_X)
				pos_x = origPosition_x + MAX_SPRING_DISTANCE_X


			disableDraw()
			document.HVAC.setY(pos_y)
			document.HVAC_Touch.setY(newPosition_y-ICON_HEIGHT_DIV2-offset_y)
			document.HVAC.setX(pos_x)
			document.HVAC_Touch.setX(newPosition_x-ICON_HEIGHT_DIV2-offset_x)
			refreshRect(0,10,457,346)

			enableDraw()
			start_y = newPosition_y//pos_y//
			start_x = newPosition_x//pos_x//
			
			drawLineAAW(-20, 100+ICON_HEIGHT_DIV2, pos_x, pos_y+ICON_HEIGHT_DIV2)
			drawLineAAW(457, 166, pos_x+ICON_WIDTH, pos_y+ICON_HEIGHT_DIV2)
		}
	}
}
	//Draw_Flags
	#define DRAW_FIRST_PIXEL	1
	#define DRAW_LAST_PIXEL	2
#define WHITE 0xFFFFFFFF
drawLineAAW(int x1, int y1, int x2, int y2)
{
	drawLine_AA(float(x1), float(y1)+0.5,float(x2),float(y2)+0.5, WHITE, 0)
}
drawPixel_f(float x, float y, int color)
	{
		color &amp;= 0x00FFFFFF  //mask off alpha
		//4 draw pixels with relative alpha
		new float x_fract = floatfract(x)
		new float y_fract = floatfract(y)
		new int x_int = floatround(x, floatround_method:floatround_floor)
		new int y_int = floatround(y, floatround_method:floatround_floor)
		
		
		new int pixel1_alpha = floatround((1.0 - x_fract) * (1.0 - y_fract) * 255.0) &lt;&lt; 24
		new int pixel2_alpha = floatround((x_fract) * (1.0 - y_fract) * 255.0) &lt;&lt; 24
		new int pixel3_alpha = floatround((1.0 - x_fract) * (y_fract) * 255.0) &lt;&lt; 24
		new int pixel4_alpha = floatround((x_fract) * (y_fract) * 255.0) &lt;&lt; 24
		
		drawPixel(x_int, y_int, pixel1_alpha + color)
		drawPixel(x_int+1, y_int, pixel2_alpha + color)
		drawPixel(x_int, y_int+1, pixel3_alpha + color)
		drawPixel(x_int+1, y_int+1, pixel4_alpha + color)
	}

	drawLine_AA(float x1, float y1, float x2, float y2, int color, int Draw_Flags)
	{
		if(x1 &gt; x2)
		{
			new float temp = x1
			x1= x2
			x2 = temp
			temp = y1
			y1 = y2
			y2 = temp
			//TODO: Swap Draw Flags
		}
		new float slopeY = (y2-y1)
		new float slopeX = (x2-x1)
		if(slopeX &gt; 0.0001)
		{
			new float slope = slopeY / slopeX
			new float max_pixels = floatpower(floatpower(slopeY,2.0) + floatpower(slopeX,2.0), 0.5)  //calculate length of hypotenuse
			new float deltaX = (x2 - x1) / max_pixels
			if(Draw_Flags &amp; DRAW_FIRST_PIXEL)
				drawPixel_f(x1, y1, color)
			new float y = y1	
			for(new float x = x1; x&lt;x2; x=x+deltaX)
			{
				y = y + slope * deltaX
				drawPixel_f(x, y, color)
			}
			if(Draw_Flags &amp; DRAW_LAST_PIXEL)
				drawPixel_f(x2, y2, color)
		}
		else //vertical line
		{
			//TODO: integrate draw flags
			drawLine(floatround(x1),floatround(y2),floatround(x2),floatround(y2),color,1)
		}
	}

@load()
{
	time = 354
	boing()
}

public toAutomotive()
{
	static int lastPos
	new int newPos = document.Automotive_scroller
	if (newPos == lastPos)
		Motor_coach_home.open()
	else
		lastPos = newPos
}
