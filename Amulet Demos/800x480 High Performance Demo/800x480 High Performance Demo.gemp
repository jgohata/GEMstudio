//radius of circle is about 575

&lt;script&gt;

//575 radius
// 0.90872685621 radians
public arc()
{
	new float angle
	new int x1 = 522
	new int y1 = 0
	new int x2
	new int y2
	for(angle = 0.90872685621; angle &gt; -0.90872685621; angle-=0.01)
	{
		x2 = -75 + floatround(575.0 * floatcos(angle))
		y2 = 240 - floatround(575.0 * floatsin(angle))
		drawLine(x1,  y1,  x2,  y2, 0xFFFFFF,1)
		x1 = x2
		y1 = y2
	}
}
	#define BLACK	0x00000000
	#define WHITE	0x00FFFFFF
	#define BLUE	0x00FF0000
	#define RED 	0x000000FF
	#define GREEN_OVERLAY 0x3F3FC03F
	
	#define ICON_SIZE 83
	#define SMALL_NUM_HEIGHT 23
	#define SMALL_NUM_WIDTH 16
	#define TENS_X_SPACING 73
	#define ONES_X_SPACING 91
	#define Y_SPACING 30
	

	static int HeatIcon_x;
	static int CoolIcon_x;
	static int touched = 0;
	static int validTouchBottom;
	static int validTouchTop;
	new float angle
	new float arcsin_lookup[480]
	//populate an arcsin lookup table since we dont have arcsin math, but the Hypotenuse is fixed and we know the opposite side
	arcsin()
	{
		new int i;
		new int j = 0
		for(i=240;i&gt;0;i--)
		{
			arcsin_lookup[j] = floatsin(float(i) / 575.0)
			j++
		}
		for(i=0;i&lt;239;i++)
		{
			arcsin_lookup[j] = floatsin(float(i) / 575.0)
			j++
		}
		
	}
	
	@load()
	{
		arcsin()
		document.HeatSlider.disappear()
		document.HeatTens.disappear()
		document.HeatOnes.disappear()
		document.CoolSlider.disappear()
		document.CoolTens.disappear()
		document.CoolOnes.disappear()
		HeatIcon_x = (65-50)*454/42//65 degrees
		CoolIcon_x = (80-50)*454/42//80 degrees
		document.HeatSlider.setY(480-HeatIcon_x)
		document.HeatTens.setY(480-HeatIcon_x+Y_SPACING)
		document.HeatOnes.setY(480-HeatIcon_x+Y_SPACING)
		document.CoolSlider.setY(480-CoolIcon_x)
		document.CoolTens.setY(480-CoolIcon_x+Y_SPACING)
		document.CoolOnes.setY(480-CoolIcon_x+Y_SPACING)
		
		new int x
		angle = arcsin_lookup[HeatIcon_x-ICON_SIZE/2]
		x = -155 + floatround(575.0 * floatcos(angle))
		document.HeatSlider.setX(x)
		document.HeatTens.setX(x+TENS_X_SPACING)
		document.HeatOnes.setX(x+ONES_X_SPACING)
		
		angle = arcsin_lookup[CoolIcon_x-ICON_SIZE/2]
		x = -155 + floatround(575.0 * floatcos(angle))
		document.CoolSlider.setX(x)
		document.CoolTens.setX(x+TENS_X_SPACING)
		document.CoolOnes.setX(x+ONES_X_SPACING)
		
		new int heatTemp = HeatIcon_x*42/454+50
		new int coolTemp = CoolIcon_x*42/454+50

		Internalram.Byte(1) = heatTemp
		Internalram.Byte(2) = coolTemp
		document.HeatOnes = heatTemp
		document.HeatTens = heatTemp
		document.CoolOnes = coolTemp
		document.CoolTens = coolTemp
		
		document.HeatSlider.reappear()
		document.HeatTens.reappear()
		document.HeatOnes.reappear()
		document.CoolSlider.reappear()
		document.CoolTens.reappear()
		document.CoolOnes.reappear()
		
		document.IndoorOnes = 73
		document.IndoorTens = 73
		
	}
	
	public determineTouch()
	{
		new int X = 480-PenY();
		//find out which icon the touch is closer to.
		if(X &lt; (HeatIcon_x + CoolIcon_x)/2)
		{
			touched = 1
			validTouchBottom = 480-ICON_SIZE
			validTouchTop = ICON_SIZE
		}
		else
		{
			touched = 2
			validTouchBottom = 480
			validTouchTop = 2*ICON_SIZE
		}
		
	}
		
	
	public slider()
	{
		new int X = 480 - PenY();
		//correct for touching center of object, but setX works on left side.
		X += ICON_SIZE/2
		//make sure new position is valid
		if (validTouchTop &lt; X &lt; validTouchBottom)
		{
			
			//clear redraw path.
			//document.TemperatureChannel.reappear()
			
			//HeatSlider is touched
			if(touched == 1)
			{
				HeatIcon_x = X
				if(X + ICON_SIZE &gt; CoolIcon_x)
				{
					CoolIcon_x = X + ICON_SIZE		
				}
			}
			//Cool Slider is touched
			else
			{
				CoolIcon_x = X
				if(X - ICON_SIZE &lt; HeatIcon_x)
				{
					HeatIcon_x = X - ICON_SIZE
				}
				
			}
			document.HeatSlider.disappear()
			document.HeatTens.disappear()
			document.HeatOnes.disappear()
			document.CoolSlider.disappear()
			document.CoolTens.disappear()
			document.CoolOnes.disappear()
			document.HeatSlider.setY(480-HeatIcon_x)
		document.HeatTens.setY(480-HeatIcon_x+Y_SPACING)
		document.HeatOnes.setY(480-HeatIcon_x+Y_SPACING)
		document.CoolSlider.setY(480-CoolIcon_x)
		document.CoolTens.setY(480-CoolIcon_x+Y_SPACING)
		document.CoolOnes.setY(480-CoolIcon_x+Y_SPACING)
			new int x
		angle = arcsin_lookup[HeatIcon_x-ICON_SIZE/2]
		x = -155 + floatround(575.0 * floatcos(angle))
		document.HeatSlider.setX(x)
		document.HeatTens.setX(x+TENS_X_SPACING)
		document.HeatOnes.setX(x+ONES_X_SPACING)
		
		angle = arcsin_lookup[CoolIcon_x-ICON_SIZE/2]
		x = -155 + floatround(575.0 * floatcos(angle))
		document.CoolSlider.setX(x)
		document.CoolTens.setX(x+TENS_X_SPACING)
		document.CoolOnes.setX(x+ONES_X_SPACING)
			new int heatTemp = HeatIcon_x*42/454+50
			new int coolTemp = CoolIcon_x*42/454+50
			Internalram.Byte(1) = heatTemp
			Internalram.Byte(2) = coolTemp
			document.HeatOnes = heatTemp
		    document.HeatTens = heatTemp
			document.CoolOnes = coolTemp
		    document.CoolTens = coolTemp
			document.HeatSlider.reappear()
		document.HeatTens.reappear()
		document.HeatOnes.reappear()
		document.CoolSlider.reappear()
		document.CoolTens.reappear()
		document.CoolOnes.reappear()
			InternalRAM.word(0) = HeatIcon_x
			InternalRAM.word(1) = CoolIcon_x

		}
		
	}
	public incTemp()
	{
		new int temp = document.IndoorOnes + 1
		if(temp &lt; 100)
		{
			document.IndoorOnes = temp
			document.IndoorTens = temp
		}
	}
	public decTemp()
	{
		new int temp = document.IndoorOnes - 1
		if(temp &gt;9)
		{
			document.IndoorOnes = temp
			document.IndoorTens = temp
		}
	}
	
&lt;/script&gt;
